/*案例列表样式*/

.case{width:1120px;margin: 20px auto; position: relative;}
.case .flex{justify-content:space-between;}
.case .bd{ position:relative; height:100%; z-index:0;   }
.case .case-content{justify-content: space-evenly; zoom:1;width:1100px;}
.case .case-content-left h1{font-size:36px;}
.case .case-content-left .age{font-size:28px;color:#999;}
.case .case-content-left .bottom-line{border-bottom:1px solid #dfdfdf;display:inline-block;margin:10px 0;padding-bottom:10px;}
.case .case-content-left .case-msg{color:#666;margin-bottom:45px;}
.case .case-content-list{width:80%;margin: 0 auto;}
.case .bottom-btn{padding:0;margin-top:6.5%;}
.case .button{display:block;margin:0 auto;padding:9px 15px;font-size:18px;background: #C0272D; text-align: center; color: #fff;}

.case .arrow{width:40px;height:40px;display:inline-block;border-bottom:3px solid #fff;border-left:3px solid #fff;position:absolute;top:0;bottom:0;margin:auto;transition:all .2s linear;}
.case .arrow-left,.case .arrow-right{background-color:#333;height:100px;width:65px;position:absolute;top: 50%;margin-top: -50px;}
.case .arrow-right,.case .arrow-left{cursor:pointer;}
.case .arrow-right{left:99%;}
.case .arrow-left{right:99%;}
.case .arrow-left .arrow{transform:rotate(45deg);right:-5%;}
.case .arrow-right .arrow{transform:rotate(225deg);left:-5%;}

