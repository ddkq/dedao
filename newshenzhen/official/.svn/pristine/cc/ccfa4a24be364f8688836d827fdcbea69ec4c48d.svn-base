.right_doctorBox .bd .info p{color: #818283; font-size: 14px;}
.right_doctorBox .bd .item{position: absolute; top: 237px; left: 0;color: #7d7d7d;padding-left: 15px; }
.right_doctorBox .bd .swt {position: absolute; top: 285px; left: 45px;}
.right_doctorBox .bd .swt a{width: 124px; height: 40px; background: #c0272d; color: #fff; line-height: 40px; text-align: center; float: left; margin-right: 1px;}
.right_doctorBox .bd .swt a:nth-child(2){background: #d2ab66;}

.contact{background: url(../images/contact_right_bg.png); height: 349px; padding: 50px; color: #fff; font-size: 16px; margin-top: 30px;}
.contact .tel{margin-bottom: 40px; display: table;}
.contact .tel i{display: table-cell; vertical-align: middle; font-size: 30px;}
.contact .tel div{display: table-cell;padding-left: 10px;}
.contact .tel div p:nth-child(2){font-size: 24px;}

.contact .time{margin-bottom: 40px; display: table;}
.contact .time i{display: table-cell; vertical-align: middle; font-size: 30px;}
.contact .time div{display: table-cell;padding-left: 10px;}

.contact a{width: 124px; height: 58px; background: #c0272d; color: #fff; line-height: 58px; text-align: center; float: left; margin-right: 1px;}
.contact a:nth-child(2){background: #d2ab66;}

.case h3{color: #232332; font-weight: bold; font-size: 18px; border-bottom: 1px solid #d0d0d0;position: relative;line-height: 30px; margin-bottom: 20px; margin-top: 20px;}
.case h3:after{content: ""; border: 1px solid #c0272d;width: 65px; position: absolute; left: 0; bottom: 0;}
.case h3 a{position: absolute;right: 0; bottom: 0;color: #d0d0d0; font-size: 12px; font-weight: 100;}

.right_caseBox{ width:338px; height:320px; overflow:hidden; position:relative; }
.right_caseBox .hd{ height:15px; overflow:hidden; position:absolute; left:150px; bottom:0px; z-index:1;}
.right_caseBox .hd ul{ overflow:hidden; zoom:1; float:left;  }
.right_caseBox .hd ul li{ float:left; margin-right:2px;  width:15px; height:15px;text-align:center; background:#717171; cursor:pointer; border-radius: 15px;}
.right_caseBox .hd ul li.on{ background:#c0272d; color:#fff; }
.right_caseBox .bd{ position:relative; height:290px; z-index:0; background: #efefef;  }
.right_caseBox .bd li{ zoom:1; vertical-align:middle; }
.right_caseBox .bd img{ width:338px; height:240px; display:block;}
.right_caseBox .bd p{padding: 5px; font-size: 16px; text-align: center; line-height: 40px;}
/*右侧样式*/