/*院内环境*/
.brand_hj_info{width: 90%; padding:1.2rem; margin: 1rem auto; font-size:0.8rem;line-height:1.6rem;border: 1px solid #d6b375; border-bottom: 0; position: relative;}
.brand_hj_info:before{content: ""; width: 3px; height: 93px; background: #d6b375; border-top: 5px solid #fff; border-bottom: 5px solid #fff; position: absolute; left: -2px; top: 54px;}
.brand_hj_info:after{content: ""; width: 3px; height: 93px; background: #d6b375; border-top: 5px solid #fff; border-bottom: 5px solid #fff; position: absolute; right: -2px; top: 100px;}
.slideBox{ position:relative; width:90%; overflow:hidden; margin:0.8rem auto; }
.slideBox .hd{ position:absolute; height:2.5rem; line-height:2.5rem; bottom:0; right:0; z-index:1; }
.slideBox .hd li{ display:inline-block; width:5px; height:5px; -webkit-border-radius:5px; -moz-border-radius:5px; border-radius:5px; background:#fff; text-indent:-9999px; overflow:hidden; margin:0 6px;   }
.slideBox .hd li.on{ background:#C0272D;  }
.slideBox .bd{ position:relative; z-index:0; }
.slideBox .bd li{ position:relative;  }
.slideBox .bd li img{ width:100%;display:block;   }
.slideBox .bd li a{ -webkit-tap-highlight-color:rgba(0, 0, 0, 0); /* 取消链接高亮 */ }
.slideBox .bd li .tit{ display:block; width:100%;  position:absolute; bottom:0; height:2.5rem; line-height:2.5rem; color:#fff; background: rgba(0,0,0,0.6);padding-left: 1rem;   }


