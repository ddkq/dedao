						$('.contact-us').css('padding', '0')
						$('.bottom-text').css('bottom', '-10%')
					}
		     	}
		     }
		})
		// 点击按钮跳到第二屏
	  	$('.next-btn').on('click', function() {
	  		mySwiper.slideTo(1, 500, false)
	  		$('#menu').css('background-color','#000')
	  		// 第二版动画效果
			setTimeout(function() {
	  			$('.section2 .section-title').css({'top': '5%', 'opacity': 1})
				setTimeout(function() {
					$('.section2 .section-text p').eq(0).css({'top': '0', 'opacity': 1});
					setTimeout(function() {
						$('.section2 .section-text p').eq(1).css({'top': '0', 'opacity': 1});
						setTimeout(function() {
							$('.section2 .section-title').removeClass('section-line');
						}, 100)
					}, 500)
				}, 400)
	  		}, 200)
  		
	  	})
	  	var mySwiper2 = new Swiper ('.section-content', {
		    simulateTouch : false,
		    navigation: {
			    nextEl: '.right-btn',
			    prevEl: '.left-btn',
			}
	  	})
	})
</script>
</html>