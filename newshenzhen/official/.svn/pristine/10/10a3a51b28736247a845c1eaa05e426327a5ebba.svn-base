/*列表页样式*/
.main_list{width:100%;margin-top: 0.6rem;}
.term_title{border-bottom:1px solid #f1ece6;overflow: hidden;color:#3a3633;padding-left: 2rem;height: 2.4rem;font-size: 1.4rem; position: relative; background: url(../images/manage_fill.png) no-repeat; background-size:2rem ; margin-left: 0.6rem;}
.term_title i.show_child{font-size: 1.4rem; position: absolute; right: 1rem; bottom: 0.5rem;}

.termchild_list{padding: 0.8rem 0 0.2rem 0; display: none;}
.termchild_list li a{ font-size: 0.8rem; display: block; line-height: 1.4rem; border: 1px solid #ccc; border-radius:5px ; text-align: center; margin-bottom: 0.5rem;}

.main_list ul{margin:0 auto;}
.main_list ul.lists li{border-bottom:1px dashed #f1ece6;width:100%;background:#fff;padding: 5%;}
.main_list ul.lists li a{color: #000; font-size: 0.8rem; display: table;}
.main_list ul.lists li p.time{color: #696969; margin: 0.3rem 0;}
.main_list ul.lists li a i{font-size: 1.4rem; width: 2rem; height: 2rem; border: 1px solid #ccc; border-radius: 1rem; text-align: center; line-height: 2rem;  }

.dlists .media-body{width: 70%;}
.dlists .media-body p{font-size: 0.6rem;}
.dlists .media-body p span{margin-right: 0.5rem;font-size: 1rem;}
.dlists .media-body .swt a{float: left; width: 5rem; height: 2rem; line-height: 2rem; color: #fff; background-color: #C0272D; border-radius: 5%; text-align: center;}
.dlists .media-body .swt a:nth-child(2){float: left; margin-left: 0.2rem; background-color: #C09853}

/*列表页样式*/

/*口腔咨询样式*/
.tszl{ margin-top:1rem; overflow:hidden;}
.tszl ul li{width: 95%;  border:1px solid #e5e5e9; margin:2rem auto; overflow:hidden;padding: 2rem 0;}
.tszl ul li .thumbnail{border: 0;margin-bottom: 0;}
.tszl ul li .tszl_img{ display:block; margin:0 auto 30px;}
.tszl ul li h3{ margin:0 auto 1rem; font-size:1rem; text-align:center; color:#1d212a;}
.tszl ul li h3 font{ display:block; font-size:1.2rem;}
.tszl ul li h3 span{ display:block; width:60%; margin:0 auto;}
.tszl ul li .tszl_li a{font-size:0.8rem; color:#616162; line-height:1.2rem; margin-bottom:0.5rem; padding: 0; text-align: center;}
.tszl ul li .tszl_li a:last-child{border: 0;}
.tszl ul li .tszl_a{ display:block; width:80%; height:2rem; line-height:2rem; margin:0.5rem auto 0 auto; border:1px solid #C09853; border-radius:5%; text-align:center; font-size:0.8rem; color:#1d212a;}
/*口腔咨询样式*/



/*终端页样式*/
.font_page{padding-bottom:1rem;padding:0.3rem 0.2rem; font-size: 0.8rem;}
.font_page_nr{width:100%;padding-bottom:0.5rem;}
.font_page_nr h3{width:100%;font-size:1.4rem;color:#4f4b4a;font-weight:bold;padding-bottom:1rem;text-align:center;}
.font_page_nr h4{width:100%;font-size:1rem;color:#4f4b4a;padding-top:1rem;text-align:center;}
.font_page_nr .font_content{width:92%;font-size:0.8rem;color:#5f5e5c;margin:0 auto;line-height:2rem;}
.font_page_nr .font_content p strong{font-weight: bold;}

.font_page_nr .media-body{width: 65%;}
.font_page_nr .media-body h3{text-align: left;}
.font_page_nr .media-body p{font-size: 0.8rem;}
.font_page_nr .media-body .swt{margin: 0.3rem 0;}
.font_page_nr .media-body .swt a{float: left; width: 4rem; height: 1.2rem; font-size: 0.6rem; line-height: 1.2rem; color: #fff; background-color: #C0272D; border-radius: 5%; text-align: center;}
.font_page_nr .media-body .swt a:nth-child(2){float: left; margin-left: 0.2rem; background-color: #C09853}

.article_btn li {line-height:1rem}
.article_btn li a { display:block; font-size:1rem; color:#fff; padding:5px;}
.article_btn li i {display:inline-block; width:2rem; height:2rem;font-size:1.8rem;  float:left;}
.article_btn li span{ font-size:0.6rem; display:block;}
.article_btn .icon_01{background:url(../images/con_zx_ico.png) 0 7px no-repeat; background-size:auto 1.5rem;}
.article_btn .icon_02{background:url(../images/con_zx_ico.png) -30px 7px no-repeat; background-size:auto 1.5rem;}
.article_btn li:nth-child(1) a{background:#C0272D}
.article_btn li:nth-child(2) a{background:#C09853;}

.related_list{ overflow:hidden;padding: 1rem;}
.related_list ul{ padding: 0 0.3rem;}
.related_list li{ height:2rem; line-height:2rem; overflow:hidden; border-bottom:1px dashed #ddd;}
.related_list li a{font-size: 0.5rem;}


/*问答终端页*/
.main_content{width: 92%; margin: 0.5rem auto;}
.question_title{font-size: 1.4rem; color: #4f4b4a; font-weight: bold;padding-bottom:0.3rem;text-align:left;}
.user_info span{color:#999;float:left;margin-right:0.4rem;padding-bottom:0.2rem; font-size: 0.8rem;}

.question_info{padding:0;}
.question_info p{overflow:hidden;clear:both;display:block;font-size:0.8rem;line-height:1.4rem;}
.question_info p span{color:#999;font-size:0.9rem;display:block; margin-top: 0.3rem;}

.answer {margin: 1.2rem 0;}
.answer_title h3 {font-size: 1.3rem ;color: #666;font-weight: bold; line-height: 2rem; border-bottom: 1px solid #C0272D; margin-bottom: 0.5rem;}

.doctor_info {font-size: 0.4rem;}
.doctor_info b{margin-left: 0.5rem;}
.doctor_info a {font-size: 0.8rem;display: block;line-height: 1.6rem;padding-top: 2px; color: #C0272D; font-weight: bold;}
.doctor_info a.swt{background-color:#FF893C;width:4rem;height:2rem;line-height:2rem;text-align:center;font-weight:bold;font-size:0.6rem;color:#fff;display:inline-block;margin-top:0.5rem;vertical-align:middle;}
.doctor_info .excerpt{font-size: 0.6rem;}

.answercont .anscontc{padding:0.5rem 1rem;border: 1px solid #d6e7c3; background: #f6ffed; margin: 1rem 0; position: relative;}
.answercont .anscontc:after{content: ""; background:url(../images/b_ansbox.png) no-repeat; position: absolute; top: -13px; width: 20px; height: 13px; }

.anscont_time{float:right;color:#999;font-size: 14px;}
.anscont_cont{overflow:hidden;clear:both;line-height:24px;font-size:14px;color:#999999;}
.anscont_cont span{display:block;}
.anscont_cont p{display:block;line-height:24px;font-size:14px;}

.question_related{margin-top:0.8rem;clear:both;zoom:1;}
.question_related h4{font-weight: bold;}
.question_related ul{overflow: hidden;zoom: 1;}
.question_related ul li {line-height: 2rem;float: left;overflow: hidden;border-bottom: 1px dashed #e3e3e3; font-size: 0.6rem;}
.question_related ul li a{color: #C0272D;}


/*百科终端页样式*/

.en_info h4{font-size: 1rem; color: #333333; font-weight: bold;}
.en_info b{font-weight: bold; font-size: 0.8rem; color: #292929;padding: 0;}
.en_info .fri_text{padding-bottom: 1rem; border-bottom: 1px dashed #ccc;}
.en_info .fri_text i{color: #c0272d;}
.en_info .excerpt span{display: block; line-height: 1.6rem;font-size: 0.8rem;}
.en_info p.text{line-height: 2rem;font-size: 0.8rem}
.en_info .swt a{width: 5rem; font-size: 0.8rem; height: 2rem; background: #c0272d; color: #fff; line-height: 2rem; text-align: center; float: left; margin-right: 0.8rem;}
.en_info .swt a:nth-child(2){background: #d2ab66;}

.step{margin-top: 2rem;}
.step .main{padding: 0 1rem;}
.step .main h2{margin:1rem 0 10px 0;font-size: 1rem; font-weight: bold; color:#C0272D;  padding-left:1.5rem ; position: relative;}
.step .main h2:after{position:absolute ; left:0; content: "";background: #C0272D;width: 1rem;height: 1.2rem;line-height: 1.2rem; padding-left: 1rem;}
.step .main h2 i{float: right;}
.step .main .text {line-height: 2rem; font-size: 0.8rem; color: #494949;}
.step .main .swt_btn{display: block; width: 8rem; height: 2rem; line-height: 2rem; color: #fff; background-color: #C0272D; border-radius: 5%; text-align: center; margin: 0 auto;}
.step .main .swt_btn i{margin-left: 3px;}

/*经验终端页*/
.gostep{display: block;font-size: 0.8rem; text-align: right; padding: 0.6rem 0;}

.summary{line-height: 1.8rem;font-size: 0.6rem;margin-bottom:1.6rem;clear: both;}
.summary span{font-size: 1rem;}

.excontent{color: #333;font-size: 1rem;line-height: 2rem;margin-top: 2.6rem;}
.excontent h3{color: rgb(51, 51, 51);padding-bottom: 0.6rem;margin: 40px 0px 0px;border-bottom: 1px solid rgb(214, 214, 216);margin-bottom: 0.6rem;font-size: 1.4rem;}
.excontent p{font-size: 0.8rem; line-height: 1.8rem;}

.step{margin-bottom:20px;clear: both;position: relative;}
.step .st{font-size: 16px;display: block;overflow: auto;}
.step .st font{display: block;color: #fff;background: #C0272D;width: 25px;text-align: center;height: 26px;line-height: 26px;float: left;margin-right: 14px;border-radius: 20px;position: absolute;left: 5px;}

.step .extext{min-height: 50px;border-left: 2px dotted #e4e4e4;margin-left: 17px;}
.step .extext .stepcontent {padding: 0 20px 20px 20px;}
.step .extext .stepcontent .img_p{position: relative;display: block;}
.step .extext .stepcontent .img_p:hover .enter_step_btn{display: block;}
.enter_step_btn {width: 120px;height: 40px;position: absolute;top: 50%;left: 50%;margin-top: -20px;margin-left: -60px;color: #fff;display: none;text-align: center;cursor: pointer;text-decoration: none;background: #333333ad; line-height: 40px; border-radius: 20px;}

/*经验分页阅读*/
.stepTabBox{width: 95%; margin: 0 auto;}
.stepTabBox h3{font-size: 1.2rem; font-weight: bold; margin-bottom: 0.5rem; }
.stepTabBox .thumbnail{border: 0;}
