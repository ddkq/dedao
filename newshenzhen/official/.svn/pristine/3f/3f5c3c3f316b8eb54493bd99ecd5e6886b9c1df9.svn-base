.section06 .anchorBL, .quad4, .anchorTR{display: none !important;}
.section06 .BMap_pop > div > div {border-radius: 8px;}
.section06 {position: relative;}
.section06 .contact-bcg {position: absolute;bottom: 0;z-index: 8;right: -50%;background: transparent;border-bottom: 950px solid #000;border-left: 350px solid transparent;border-right: 0px solid transparent;width: 45%;height: 100%;transition: all 0.26s ease 300ms;transition-duration: 1.2s;}
.section06 .contact-us {    position: absolute;top: 50%;z-index: 9;width: 25%;text-align: right;color: #fff;right: 80px;transform: translateY(-50%);}
.section06 .contact-us .title {opacity: 1;transform: translateY(0);color: #fff;margin-bottom: 20px;height: 60px;font-size: 32px;position: relative;margin: 0;font-weight: normal;}
