第一屏*/
.slide video, .slide img {position: relative;z-index: -99999;object-fit: cover;}
.slide .banner-text {width: 100%;height: 100%;position: absolute;left: 0;top: 0;}
.slide .banner2-text {width: 100%;height: 100%;position: absolute;top: 0;}
.slide .banner-text-con{position: absolute; width: 80%; top: 40%; left: 10%; color: #fff;-webkit-animation:textX 2s;-moz-animation:textX 2s;-o-animation:textX 2s;-ms-animation:textX 2s;animation:textX 2s}
.slide .banner-text-con .title{margin-bottom:15px;font-size:3em;height:auto;color: #fff;font-weight: 600;}
.slide .banner-text-con .subtitle{margin-top:15px;font-size:1.6em;color: #fff;}
.slide .line {width: 100px;height: 4px;background-color: #c11920;display: block;margin: 0 auto;}
.banner-next{z-index:99;position:absolute;bottom:50px;left:50%;-webkit-transform:translateX(-50%);-moz-transform:translateX(-50%);-o-transform:translateX(-50%);-ms-transform:translateX(-50%);transform:translateX(-50%);cursor:pointer}
.banner-next a{position:relative;display:block;width:26px;height:70px}
.banner-next a img{position:absolute;-webkit-transition:all 0.5s;-moz-transition:all 0.5s;-ms-transition:all 0.5s;-o-transition:all 0.5s;transition:all 0.5s}
.banner-next a .mouse{position: unset; -webkit-animation:bounceY 2s infinite;-moz-animation:bounceY 2s infinite;-o-animation:bounceY 2s infinite;-ms-animation:bounceY 2s infinite;animation:bounceY 2s infinite}
.banner-next a .arrows-1{-webkit-transform:rotate(90deg);-moz-transform:rotate(90deg);-o-transform:rotate(90deg);-ms-transform:rotate(90deg);transform:rotate(90deg);left:10px;top:50px;-webkit-animation:fade 2s infinite;-moz-animation:fade 2s infinite;-o-animation:fade 2s infinite;-ms-animation:fade 2s infinite;animation:fade 2s infinite}
.banner-next a .arrows-2{-webkit-transform:rotate(90deg);-moz-transform:rotate(90deg);-o-transform:rotate(90deg);-ms-transform:rotate(90deg);transform:rotate(90deg);left:9px;top:53px;-webkit-animation:fade 2s infinite 1s;-moz-animation:fade 2s infinite 1s;-o-animation:fade 2s infinite 1s;-ms-animation:fade 2s infinite 1s;animation:fade 2s infinite 1s}

@-webkit-keyframes textX{
	0%{transform:translateY(0px)}
	50%{transform:translateY(60px)}
	100%{transform: translateY(0px);}
}
@-moz-keyframes textX{
	0%{transform:translateY(0px)}
	50%{transform:translateY(60px)}
	100%{transform: translateY(0px);}
}
@-o-keyframes textX{
	0%{transform:translateY(0px)}
	50%{transform:translateY(60px)}
	100%{transform: translateY(0px);}
}
@keyframes textX{
	0%{transform:translateY(0px)}
	50%{transform:translateY(-60px)}
	100%{transform: translateY(0px);}
}


@-webkit-keyframes bounceY{
	0%,100%{-webkit-transform:translateY(0);transform:translateY(0)}
	50%{-webkit-transform:translateY(-15px);transform:translateY(-15px)}
}
@-moz-keyframes bounceY{
	0%,100%{-moz-transform:translateY(0);transform:translateY(0)}
	50%{-moz-transform:translateY(-15px);transform:translateY(-15px)}
}
@-o-keyframes bounceY{
	0%,100%{-o-transform:translateY(0);transform:translateY(0)}
	50%{-o-transform:translateY(-15px);transform:translateY(-15px)}
}
@keyframes bounceY{
	0%,100%{transform:translateY(0)}
	50%{transform:translateY(-15px)}
}
@-webkit-keyframes fade{
	0%,100%{opacity:0}
	50%{opacity:1}
}
@-moz-keyframes fade{
	0%,100%{opacity:0}
	50%{opacity:1}
}
@-o-keyframes fade{
	0%,100%{opacity:0}
	50%{opacity:1}
}
@keyframes fade{
	0%,100%{opacity:0}
	50%{opacity:1}
}

/*公共*/
.btn{ position: absolute;}
.btn a{display: block;width: 10em; border: 1px #c11920 solid; margin: 0 auto; font-size: 0.8em;padding: 0.4em 0;color: #c11920; position: relative;text-align: center;transition: all 0.3s linear 0s;}
.btn .add {position: absolute;width: 1rem;height: 1rem;background-color: #fff;top: -.57rem;right: .437rem;font-style: normal;}
.btn a:hover {transform: translate3d(0, -2px, 0);box-shadow: 0 15px 30px rgba(0, 0, 0, 0.1);}
.section .section-line:before,.section .section-line:after {width: 0;}


/*第二屏*/
.section h2{font-size: 2.2em; font-weight: bold;display: table; margin: 0 auto;padding-top: 7%; padding-bottom: 0.8em; position: relative;}
.section h2 p{font-size: 0.6em;font-weight: 100;padding: 0.8em 0; color: #666;}
.section h2 i{position: absolute; background: url(../images/heart.png); width: 21px; height: 19px; bottom: 10px; left: 50%; margin-left: -10.5px;}
.section h2:before{content: " "; width: 42%; height: 1px; background: #D1D1D3; position: absolute; left: 0; bottom: 20px;transition: all .8s ease;}
.section h2:after{content: " "; width: 42%; height: 1px; background: #D1D1D3; position: absolute; right: 0; bottom: 20px;transition: all .8s ease;}
.section2 .content p{font-size: 0.9em; width: 90%; margin: 0.8em .5rem;line-height: 1.5;color: #282830;}
.section2 .content p b{color: #c11920;}
.section2 .content img.service_img{height: 30em;}
#section-3{background: #f1f1f1;}
.section2 .content ul{margin: 30px auto 0 auto; width: 80%; display: flex; justify-content: space-around;overflow: hidden;}
.section2 .content ul li{width: 23%; float: left; margin: 0 2em;background: #fff; padding: 2rem 1em;min-height: 28em;position: relative;transition: all 0.75s ease 0s;    top: 28rem;}
.section2 .content ul li:hover {transform: translate3d(0, -8px, 0);}
.section2 .content ul li h3{font-size: 2em;position: relative;font-family: 'SimSun';color: #282830;}
.section2 .content ul li h3:before{content: " "; width: 30%; height: 1px; background: #e5e5e5; position: absolute; left: 0; top: 50%;}
.section2 .content ul li h3:after{content: " "; width: 30%; height: 1px; background: #e5e5e5; position: absolute; right: 0; top: 50%;}
.section2 .content ul li h5{margin: 0.3em 0;color: #666;}
.section2 .content ul li img{margin: 0.6em 0;}
.section2 .content .btn{left: 0;right: 0;bottom: 7%;}
.section2 .section2-title {position: relative;top: 5%;transition: all .75s ease 0s;opacity: 0;}
.section2 .section2-text p {position: relative;top: 1rem;transition: all .75s ease 0s;opacity: 0;color: #848484;margin: 0.8em auto;}

.section3 .section3-title{background: url(../images/tech_title_bg.jpg);display: block;}
.section3 h2 {color: #fff;padding-bottom: 1em;transition: all .75s ease 0s;}
.section3 ul{position: relative;margin-top: 1em;height: 65%;}
.section3 ul:before{content: " "; width: 1px; height: 100%; background: #cacac8; position: absolute; left: 50%;}
.section3 ul li{position: absolute;padding: 0 2em ;transition: all .75s ease 0s;opacity: 0;}
.section3 ul li:first-child:after {top:0;}
.section3 ul li:after{content: '';display: table;width: 10px;height:10px;background: #cacac8;border-radius: 50%;position: absolute; top: 40px;}
.section3 ul li:nth-child(odd):after{left: -0.3em;}
.section3 ul li:nth-child(even):after{right: -0.3em; top: 50%;}
.section3 ul li:last-child:after{bottom: 0; right: -0.3em;top: auto;}

.section3 ul li h4{font-size: 2.5em; display: table; border-bottom: 1px solid #CACAC8; margin-bottom: 0.2em; color: #cacac8;}
.section3 ul li:nth-child(even) h4{float: right;}
.section3 ul li p{font-size: 1em;color: #666;}
.section3 ul li:nth-child(even) p{float: right; clear: both;}
.section3 ul li:first-child{left: 70%; top: 0;}
.section3 ul li:nth-child(2){right: 70%; top: 4em;}
.section3 ul li:nth-child(3){left: 70%; top: 6em;}
.section3 ul li:nth-child(4){right: 70%; top: 11em;}
.section3 ul li:nth-child(5){left: 70%; top: 13em;}
.section3 ul li:nth-child(6){right: 70%; top: 18em;}
.section3 ul li:nth-child(7){left: 70%; top: 21em;}
.section3 ul li:nth-child(8){right: 70%; top: 26em;}
.section3 ul li:nth-child(9){left: 70%; top: 28em;}
.section3 ul li:nth-child(10){right: 70%; bottom: 0;}
.section3 .num {font-family: 'SimSun'}
.section3 .list-img-box {position: relative;}
.section3 .list-img-box img {position: absolute;}
.section3 ul li:nth-child(2) img, .section3 ul li:nth-child(10) img{right: 100%; bottom: 0;}
.section3 ul li:nth-child(5) img {left: 100%;bottom:0;}
.section3 ul li img {padding: 0 5%;}
.section03-title p {color: #fff !important;}

#section-5{background-color: #f1f1f1 ;}
.map{ position:relative;width:856px; margin:0 auto;transition: all 1.15s ease;transform: scale(0);}
.map a{ color:#fff; font-size:0.8em; text-decoration:none;}
.map a:hover{color:#fff;}
.citybg{ position:absolute; display:none; z-index:3; }
