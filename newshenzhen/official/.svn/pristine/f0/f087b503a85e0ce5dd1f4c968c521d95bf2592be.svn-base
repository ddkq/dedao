	// 导航栏 三级菜单
	$(function() {
		var bannerHeight = $('.banner li img').height()
		$('.nav-porject-name').css('height', bannerHeight)
		$('.third-nav').css('height', bannerHeight)
		$('.nav .nav-tab:first-child').on('mouseenter', function() {
			$('.nav-porject-name').slideDown()
			$('.nav-porject-name .second-nav:not(:first-child) .third-nav').addClass('unshow')
			setTimeout(function() {
				$('.third-nav').animate({opacity: '1', left: '100%'}, 1000)
			}, 100)
		})
		$('.second-nav').on('mouseenter', function() {
			$(this).children('p').addClass('nav-porject-active').parent().siblings().children().removeClass('nav-porject-active')
			$(this).children('.third-nav').removeClass('unshow').parent().siblings().children('.third-nav').addClass('unshow')
		})
		$('.nav .nav-tab:first-child').on('mouseleave', function() {
			$('.third-nav').animate({opacity: '0', left: '-100%'}, 1000)
			setTimeout(function() {
				$('.second-nav>p').removeClass('nav-porject-active')
				$('.nav-porject-name').slideUp().children('.second-nav').eq(0).children('p').addClass('nav-porject-active')
				$('.third-nav').addClass('unshow').parent('.second-nav').eq(0).children('.third-nav').removeClass('unshow')
			}, 200)
		})
	})
	// banner下移入事件
	$(function() {
		$('.banner-bottom li').on('mouseenter', function() {
			$('.banner-bottom li').removeClass('banner-bottom-list-active')
			$(this).addClass('banner-bottom-list-active')
		})
		$('.content-list-box .content-list').on('mouseenter', function() {
			$('.content-list-box .content-list').removeClass('content-list-active')
			$(this).addClass('content-list-active')
		})
	})
