@charset "utf-8";
html, body, div, span, applet, object, iframe,h1, h2, h3, h4, h5, h6, p, blockquote, pre,a, abbr, acronym, address, big, cite, code,del, dfn, rem, img, ins, kbd, q, s, samp,small, strike, sub, sup, tt, var, u, i, center,dl, dt, dd, ol, ul, li,fieldset, form, label, legend,table, caption, tbody, tfoot, thead, tr, th, td,article, aside, canvas, details, rembed,figure, figcaption, .footer, .header, hgroup,menu, nav, output, ruby, section, summary,time, mark, audio, video{margin: 0;padding: 0;border: 0;font: inherit;-webkit-appearance: none;}
article, aside, details, figcaption, figure,.footer, .header, hgroup, menu, nav, section {display: block;}
ol, ul {list-style: none;}
blockquote,q {quotes: none;}
a{text-decoration: none;}
blockquote:before, blockquote:after,q:before, q:after {content: ''; content: none;}
table {border-collapse: collapse;border-spacing: 0;}
a{text-decoration: none;color: #585858;}
*{outline: none;-webkit-tap-highlight-color:rgba(255,0,0,0);}
a:focus,a:active,a:link,a:visited,a:hover{outline: none;text-decoration: none;}
input{outline: none;-webkit-tap-highlight-color:rgba(255,0,0,0);text-decoration: none;color: #585858;}
select{color:#585858;}
img{display: inline-block;max-width:100%;}
em{font-style:normal}
html{font-size: 16px;}
body{font-family:"微软雅黑";background: #fff;color: #585858;max-width:640px; margin: 0 auto;}

@font-face{font-family:SIYUAN;src: url("SOURCEHANSANSCN-EXTRALIGHT_1.OTF");}

/* 头部和底部公用部分 */
.header{width:100%; max-width:640px; height:66px; background:#fff; position:fixed; z-index:99;}
.header .logo a img{display:block; margin: 5px auto;}
.header .submenu{ position:absolute; left: 1rem;top: 1rem;line-height: 2rem;font-size: 1.6rem;color:#ccc;}
.header .submenu .fa-icon:before{content:"\f0c9";}
.header .menu_on .fa-icon:before{content:"\f00d";}
.header .menu_box{width:100%;height:100%;overflow: auto; position:absolute; right:-100%; top:0px; z-index:98;}

.header .header_phone{position: absolute; right: 1rem;top: 1rem;line-height: 2rem;font-size: 1.6rem;transform: scale(-1,1);}
.header .header_phone a{color: #ccc;}

.article_header{width:100%; max-width:640px; height:43px; background:#000; position:fixed; z-index:99;}
.article_header h1{font-size: 1rem; color: #fff; line-height: 43px;}
.article_header h1 a{margin-right: 0.5rem;color: #fff;}
.article_header h1 a i{margin-right: 0.3rem;}
.article_header .share{height: 43px; line-height: 43px;font-size: 1.6rem;color:#fff; text-align: right;}
.article_header .submenu{height: 43px; line-height: 43px;font-size: 1.6rem;color:#fff; text-align: right;}
.article_header .submenu .fa-icon:before{content:"\f0c9";}
.article_header .menu_on .fa-icon:before{content:"\f00d";}
.article_header .menu_box{width:100%;height:100%;overflow: auto; position:absolute; right:-100%; top:0px; z-index:98;}


/*头部右侧导航2018.4.4*/
.menu-head {position: relative;height: 4.125rem;background: #333;font-size: 1.2rem;color: #999;}
.menu-head h1 {line-height: 4.125rem;text-align: center;font-weight: normal;}
.menu-head-close {position: absolute;left: 1rem;top: 1rem;line-height: 2rem;font-size: 1.6rem;}
.menu-head-close .fa-icon:before{content:"\f00d";}
.menu-tabs{height: 3rem;text-align: center;line-height: 3rem;background: #e5e5e5;}
.menu-tabs li{clear: both;display: inline-block;width: 4rem;height: 2rem;margin: 0 1rem;background: #ccc;border-radius: 3px;overflow: hidden;vertical-align: middle;cursor: pointer;}
.menu-tabs li a{font-size: 1rem;color: #333;line-height: 2rem;display: block;}
.menu-tabs li.on {background: #c0262d;color: #fff;}
.menu-tabs li.on a{color: #fff;}

.menu-tab-con {height: 100%;background: #f8f8f8;display: table-cell;vertical-align: top;}

.menu-tab-left {height: 100%;background: #333;padding: 0;}
.menu-tab-left li{display: block;height: 4rem;line-height: 4rem;text-align: center;}
.menu-tab-left li.active{background: #4e4f51;}
.menu-tab-left li a{font-size: 1rem;color: #fff;}
.menu-tab-left li.active a{color: #cda255; font-weight: bold;}

.menu-tab-right{background: #f8f8f8;}
.menu-tab-right .menu-pro dl{padding: 0.5rem 0 0 0;}
.menu-tab-right .menu-pro dt{height: 2rem;line-height: 2rem;font-size: 1rem;color: #333;border-bottom: 1px solid #e1e1e1;}
.menu-tab-right .menu-pro dd{padding: 0.5rem 0 0 0;overflow: hidden;}
.menu-tab-right .menu-pro li{float: left;font-size: 0.8rem;padding: 0.5rem;line-height: 1.1rem;}
.menu-tab-right .menu-pro li a{display: block;}

.menu-tab-right .menu-doc{padding: 0.5rem 0 0 0;}
.menu-tab-right .menu-doc li{float: left;width: 50%;padding: 0.5rem 0.25rem 0.25rem 0.25rem;text-align: center;}
.menu-tab-right .menu-doc li a{display: block;}
.menu-tab-right .menu-doc li img {display: block;width: 100%;}
.menu-tab-right .menu-doc li .zj-name {margin: 0.5rem 0 0 0;font-size: 0.8rem;color: #333;line-height: 1rem;}
.menu-tab-right .menu-doc li .zj-zhichen {font-size: 0.8rem;color: #999;line-height: 1.9rem;height: 1.9rem;}
/*头部右侧导航2018.4.4*/

/*尾部*/
.footer{width: 100%;height: 3.125rem; background: #000;position: fixed;bottom: 0;z-index: 90;max-width: 640px;}
.footer a{padding: 0; color: #fff; font-size: 0.3rem; text-align: center;padding-top: 0.4rem; width: 20%;}
.footer a i{display: block;font-size: 1.5rem;}
.footer a:nth-child(4) i{animation:runnav 3000ms steps(1,end) infinite 0s; position: relative;}
@keyframes runnav {	
	0%{color:#fff;}10%{color:#ff921e;}20%{color:#fff;}30%{color:#ff921e;}40%{color:#fff;}50% {color:#ff921e;}60%{color:#fff;}70%{color:#ff921e;}80%{color:#fff;}90%{color:#ff921e;}100%{color:#fff;}	
}
.footer a:nth-child(4) i span{position: absolute; top: 0rem; right: 1rem; width: 1rem;height: 1rem; font-size: 0.6rem;color: #fff; line-height: 1rem; text-align: center; background: #e60012; border-radius: 50%;}

/*尾部*/



/*页码  */
.page{width:auto; overflow:hidden; text-align:center;padding-top: 2rem;}
.page ul{margin:0 auto;}
.page li{border-radius: 100%; display:inline-block; font-size:0.6rem;line-height:1.2rem; margin-right:1rem; text-align:center;}
.page li a{display:block; color:#000;}
.page li a:hover{border-radius: 100%;color:#000;}

/*向上按钮*/
.btn-top{font-size:26px;background:rgba(230,230,230,0.9); width:46px; height:46px; text-align:center; line-height:46px;z-index:100;display:none;position:fixed;bottom:10%;right:2%;border-radius:50%;}  

