/*主页样式*/
.more a{color: #fff;}
/*轮播图*/
.focus {width: 100%;margin: 0 auto;position: relative;overflow: hidden;}
.focus .hd {width: 100%;position: absolute;z-index: 1;bottom: 0.1rem;text-align: center;}
.focus .hd ul {display: inline-block;height: 0.625rem;padding: 3px 5px;-webkit-border-radius: 5px;-moz-border-radius: 5px;border-radius: 0.625rem;font-size: 0;vertical-align: top;}
.focus .hd ul li {display: inline-block;width: 0.625rem;height: 0.625rem;-webkit-border-radius: 5px;-moz-border-radius: 5px;border-radius: 0.625rem;background: #000000;margin: 0 5px;vertical-align: top;overflow: hidden;}
.focus .hd ul li.on{background: #C0272D;}
.focus .bd {position: relative;z-index: 0;}
.focus .bd li img {width: 100%;height: auto;vertical-align: top;}
/*轮播图*/

/*4图标*/
.index_icon{margin-top: 0.8rem;padding: 0.625rem 1rem; border-top: 1px #e6e6e6 solid; border-bottom: 1px #e6e6e6 solid;}
.index_icon li{background: #d5d1d1;padding:0; padding-top: 1rem;}
.index_icon li.bg2{background: #ecebeb;}
.index_icon li .icon_bg{background: url(../images/icon.png);width: 2.75rem; height: 2.5rem; margin: 0 auto;}
.index_icon li span{display: block; text-align: center; font-size: 0.9rem; margin: 0.5rem 0;color: #585858;}

.index_icon li .icon_bg2{background-position: -148px 0;}
.index_icon li .icon_bg3{background-position: -296px 0;}
.index_icon li .icon_bg4{background-position: -444px 0;}
/*4图标*/

/*诊疗服务*/
.index_service{padding: 0.625rem 1rem;}
.index_service h2{font-size: 1.4rem; font-weight: bold; text-align: center; width: 40%; line-height: 2.4rem; margin: 0 auto; border-bottom: 1px solid #c1c1c1;position: relative; margin-bottom: 1rem;}
.index_service h2 span{width:20%;border-bottom: 1px solid #000;position: absolute;bottom: 0;left: 50%;margin-left: -10%;}

.index_service .tabBox{overflow: hidden;}
.index_service .tabBox .hd{margin-bottom: 1rem;}
.index_service .tabBox .hd ul{.clearfix();}
.index_service .tabBox .hd li{width: 50%;height: 2.5rem; line-height: 2.5rem; background: #e1e1e1; text-align: center;float: left;position: relative;}
.index_service .tabBox .hd li.on{background: #d3ad6b;}
.index_service .tabBox .hd li.on a{color: #fff;}
.index_service .tabBox .hd li.on span{width: 0;height: 0;border-left: 10px solid transparent;border-right: 10px solid transparent;border-top: 10px solid #da3333; position: absolute; bottom: -5px;left: 50%; margin-left: -5%;}

.index_service .tabBox .bd ul{border: 1px solid #ccc;}
.index_service .tabBox .bd li{padding: 0 0.5rem; height: 8rem; position: relative;}
.index_service .tabBox .bd li span{width: 3.5rem; height: 3.5rem;background-size: 3.5rem;display: block;margin: 0 auto;margin-top: 1rem;}
.index_service .tabBox .bd li i{width: 1rem; height: 1.25rem;background: url(../images/icon.png);background-position: 0 -119px;position: absolute;top: 2px; right: 2px;}
.index_service .tabBox .bd li p{text-align: center; font-size: 0.6rem; color: #999999;}
.index_service .tabBox .bd li p.s_title{font-size: 0.8rem; color: #000;margin-top: 1rem;}

/*服务项目*/
.index_service .tabBox .bd .service li.s_frist{height: 5rem; padding: 0.7rem 0 0 1.3rem; border-bottom: 1px #ccc solid;}
.index_service .tabBox .bd .service li.s_frist:after {content: '';position: absolute;top: 0px;right: 0;height: 90%;width: 1px;background-color: #ccc;}
.index_service .tabBox .bd .service li.s_frist span{background-image:url(../images/index/service_icon01.jpg);float: left;margin: 0;}
.index_service .tabBox .bd .service li.s_second{float: right;height: 10rem;}

.index_service .tabBox .bd .service li.s_second span{background-image:url(../images/index/service_icon02.jpg);margin-top: 1.5625rem;}
.index_service .tabBox .bd .service a:nth-child(3) li{border-bottom: 1px solid #ccc;}
.index_service .tabBox .bd .service a:nth-child(3) li span{background-image:url(../images/index/service_icon03.jpg);}
.index_service .tabBox .bd .service a:nth-child(3) li:after {content: '';position: absolute;top: 5%;right: 0;height: 80%;width: 1px;background-color: #ccc;}
.index_service .tabBox .bd .service a:nth-child(4) li{border-bottom: 1px solid #ccc;}
.index_service .tabBox .bd .service a:nth-child(4) li span{background-image:url(../images/index/service_icon04.jpg);}
.index_service .tabBox .bd .service a:nth-child(4) li:after {content: '';position: absolute;top: 10%;right: 0;height: 45%;width: 1px;background-color: #ccc;}
.index_service .tabBox .bd .service li.li_title{height: 3rem;background: #d3ad6b; color: #fff;}
.index_service .tabBox .bd .service li.li_title img{width: 100%;height: 3rem;}

.index_service .tabBox .bd .service a:nth-child(6) li span{background-image:url(../images/index/service_icon05.jpg);}
.index_service .tabBox .bd .service a:nth-child(6) li:after {content: '';position: absolute;top: 10%;right: 0;height: 80%;width: 1px;background-color: #ccc;}
.index_service .tabBox .bd .service a:nth-child(7) li span{background-image:url(../images/index/service_icon06.jpg);}
.index_service .tabBox .bd .service a:nth-child(7) li:after {content: '';position: absolute;top: 10%;right: 0;height: 80%;width: 1px;background-color: #ccc;}
.index_service .tabBox .bd .service a:nth-child(8) li span{background-image:url(../images/index/service_icon07.jpg);}
/*服务项目*/

/*牙齿症状*/
.index_service .tabBox .bd .symptom li span{background: url(../images/icon.png) no-repeat;}
.index_service .tabBox .bd .symptom a:nth-child(1) li span{background-position: 0rem -9.875rem;}
.index_service .tabBox .bd .symptom a:nth-child(1) li:after {content: '';position: absolute;top: 5%;right: 0;height: 80%;width: 1px;background-color: #ccc;}
.index_service .tabBox .bd .symptom a:nth-child(2) li span{background-position: -3.5rem -9.875rem;}
.index_service .tabBox .bd .symptom a:nth-child(2) li:after {content: '';position: absolute;top: 5%;right: 0;height: 80%;width: 1px;background-color: #ccc;}
.index_service .tabBox .bd .symptom a:nth-child(3) li span{background-position: -7rem -9.875rem;}
.index_service .tabBox .bd .symptom a:nth-child(3) li:after {content: '';position: absolute;top: 5%;right: 0;height: 80%;width: 1px;background-color: #ccc;}
.index_service .tabBox .bd .symptom a:nth-child(4) li{height: 5rem; padding: 0.7rem 0 0 0.5rem; border-top: 1px #ccc solid;border-bottom: 1px #ccc solid;}
.index_service .tabBox .bd .symptom a:nth-child(4) li span{background-position: -10.5rem -9.875rem;float: left;margin: 0;}
.index_service .tabBox .bd .symptom a:nth-child(4) li:after {content: '';position: absolute;top: 5%;right: 0;height: 80%;width: 1px;background-color: #ccc;}
.index_service .tabBox .bd .symptom a:nth-child(5) li{height: 5rem; padding: 0.7rem 0 0 0.5rem; border-top: 1px #ccc solid;border-bottom: 1px #ccc solid;}
.index_service .tabBox .bd .symptom a:nth-child(5) li span{background-position: -14rem -9.875rem;float: left;margin: 0;}
.index_service .tabBox .bd .symptom a:nth-child(6) li span{background-position: -17.5rem -9.875rem;}
.index_service .tabBox .bd .symptom a:nth-child(6) li:after {content: '';position: absolute;top: 5%;right: 0;height: 80%;width: 1px;background-color: #ccc;}
.index_service .tabBox .bd .symptom a:nth-child(7) li span{background-position: -21rem -9.875rem;}
.index_service .tabBox .bd .symptom a:nth-child(7) li:after {content: '';position: absolute;top: 5%;right: 0;height: 80%;width: 1px;background-color: #ccc;}
.index_service .tabBox .bd .symptom a:nth-child(8) li span{background-position: -24.5rem -9.875rem;}
.index_service .tabBox .bd .symptom a:nth-child(8) li:after {content: '';position: absolute;top: 5%;right: 0;height: 80%;width: 1px;background-color: #ccc;}
/*牙齿症状*/
.index_service .more{width: 15.625rem;height: 2.2rem; background: #c0262d; color: #fff; text-align: center;line-height: 2.2rem;margin: 0.9375rem auto;}
/*诊疗服务*/


/*美齿见证*/
.index_case{padding: 0.625rem 1rem;}
.index_case h2{font-size: 1.4rem; font-weight: bold; text-align: center; width: 40%; line-height: 2.4rem; margin: 0 auto; border-bottom: 1px solid #c1c1c1;position: relative; margin-bottom: 1rem;}
.index_case h2 span{width:20%;border-bottom: 1px solid #000;position: absolute;bottom: 0;left: 50%;margin-left: -10%;}

.index_case .tabBox {overflow: hidden;}
.index_case .hd {width: 100%; text-align: center;}
.index_case .hd ul {display: inline-block;height: 0.625rem;padding: 3px 5px;-webkit-border-radius: 5px;-moz-border-radius: 5px;border-radius: 0.625rem;font-size: 0;vertical-align: top;}
.index_case .hd ul li {display: inline-block;width: 0.625rem;height: 0.625rem;-webkit-border-radius: 5px;-moz-border-radius: 5px;border-radius: 0.625rem;background: #333333;margin: 0 5px;vertical-align: top;overflow: hidden;}
.index_case .hd ul li.on{background: #000;}

.index_case .bd a{background: #f1f1f1;}
.index_case .bd a .case_info{padding-left: 0.8rem; width: 50%;}
.index_case .bd a .case_info span{height: 1.6rem;background: #c2996d;color: #fff;text-align: center;line-height: 1.6rem;display: block;}
.index_case .bd a .case_info p{font-size: 0.8rem;}
.index_case .bd a .case_info p.name{font-size: 1.6rem; margin-top: 0.6rem;}
.index_case .bd a .case_info font{width: 5rem;height: 1.6rem;background: #666666;color: #fff;text-align: center;line-height: 1.6rem;display: block; margin: 0.6rem 0 0.2rem 0;}


.index_case .bd a dl{clear: both;}
.index_case .bd a dl dd{padding: 0 0.125rem;margin: 0.3rem 0;}

.case_title{text-align: center;font-size: 0.8rem; line-height: 2rem;margin-bottom: 0.2rem;}
.index_case .more{width: 15.625rem;height: 2.2rem; background: #c0262d; color: #fff; text-align: center;line-height: 2.2rem;margin: 0.9375rem auto;}
