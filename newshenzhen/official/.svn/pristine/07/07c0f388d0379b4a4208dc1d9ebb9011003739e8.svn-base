/*地图*/
.index_map{background: url(../images/map.jpg);height: 10.9375rem;background-size: 100% 10.9375rem;padding: 1.5rem;position: relative;}
.index_map .media{position: absolute;left: 50%;}
.index_map .media:nth-child(2){top: 3rem;}
.index_map .media i{font-size: 1.4rem;color: #1e1e1e;}
.index_map .media .media-left{min-width: 1.875rem; text-align: center;}
.index_map .media .media-body{font-size: 0.5rem;color: #333333;}
.index_map a{width:6rem;line-height 1.25rem;background: #1e1e1e; color: #fff; font-size: 0.6rem;text-align: center; border-radius: 5px;padding: 0.4rem; position: absolute; top: 7.5rem; left: 50%;}
.index_map a i{margin-left: 0.5rem;}
.index_map .copyright{display: block; text-align: center; color: #000; margin-top: 9rem; font-size: 0.5rem; padding-bottom: 4rem;}
