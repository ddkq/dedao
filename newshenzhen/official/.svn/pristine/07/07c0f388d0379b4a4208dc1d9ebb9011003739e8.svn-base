/*齿牙名医团*/
.index_doctor{background:#e3e6eb;}
.index_doctor h2{font-size: 1.4rem; font-weight: bold; text-align: center; width: 40%; line-height: 2.4rem; margin: 0 auto; border-bottom: 1px solid #c1c1c1;position: relative; margin-bottom: 1rem;}
.index_doctor h2 span{width:20%;border-bottom: 1px solid #000;position: absolute;bottom: 0;left: 50%;margin-left: -10%;}

