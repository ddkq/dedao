/*电话*/
.index_phone{width:15rem;height: 3.125rem;background: #c0262d;border-radius: 4px;margin: 1.25rem auto;padding: 0.3rem 1.875rem;}
.index_phone img{float: left;width: 1.875rem;height: 1.875rem;margin-top: 0.3rem;}
.index_phone .phone_right{float: right;}
.index_phone .phone_right p{color: #fff;}
.index_phone .phone_right p:nth-child(1){font-family: MicrosoftYaHeiLight;font-size: 0.625rem;letter-spacing:0.3125rem}
.index_phone .phone_right p:nth-child(2){font-family: Impact; font-size: 1.4375rem;}
