.teeth h2 p{width: 300px; position: absolute; top: 45%; left: 50%; margin-left: -150px; font-size: 22px; color: #999999; opacity: 1;border-bottom: 1px solid #999999;}
.teeth h2 p span{font-size: 26px; color: #333333;border-bottom: 1px solid #C0272D; font-weight: bold;}
.teeth h2 b{width: 300px; display: block; text-align: center; font-weight: bold; position: absolute; top: 65%; left: 50%; margin-left: -150px; font-size: 16px; color: #999999; opacity: 1;}

.teeth ul{padding-left: 30px;}
.teeth li{width: 213px; height: 244px; float: left; margin-right: 5px; position: relative;}
.teeth li:last-child{margin-right: 0;margin-top: 57px;}
.teeth li:nth-child(2){margin-top: 57px;}
.teeth li:nth-child(3){height: 500px; margin-top: -50px;}
.teeth li .hide_hover{width: 213px; height: 244px; position: absolute; top: 0; left: 0; background:rgba(0,0,0,0.7); text-align: center; padding-top: 75px; display: block;}
.teeth li .teeth_li{position: relative;}
.teeth li .hide_hover h5{font-size: 16px; color: #fff;  font-weight: bold;}
.teeth li .hide_hover p{font-size: 14px; color: #999999;  position: relative; margin: 0 auto;}

.teeth li .hide_hover i{width: 80px; height: 1px; display: block; background: #fff; margin: 0 auto; position: relative;margin-top: 5px;}
.teeth li .hide_hover i:after{width: 17px; height: 1px; content: ""; background: #C0272D; position: absolute; bottom: 0; left: 50%; margin-left: -8px;}
.teeth li:nth-child(1):hover .hide_hover{display: none;}
.teeth li:nth-child(2):hover .hide_hover{display: none;}
.teeth li:nth-child(4):hover .hide_hover{display: none;}
.teeth li:nth-child(5):hover .hide_hover{display: none;}
.teeth li .teeth_li:hover .hide_hover{display: none;}

