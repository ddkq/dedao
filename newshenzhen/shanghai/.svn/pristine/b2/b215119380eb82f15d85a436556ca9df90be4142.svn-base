*{
	margin: 0;
	text-decoration: none;
    font-family: '微软雅黑';
}
ol, ul {
	list-style: none;
	padding: 0;
}
a {
	display: block;
}
img {
	max-width: 100%;
    vertical-align: middle;
}
section {
	position: relative;
	color: #fff;
}
body {
	background-color: #08257C;
}
.center {
	text-align: center;
}
.flex {
	display: flex;
    display: -webkit-box;  /* 老版本语法: Safari, iOS, Android browser, older WebKit browsers. */
    display: -moz-box;     /* 老版本语法: Firefox (buggy) */
    display: -ms-flexbox;  /* 混合版本语法: IE 10 */
    display: -webkit-flex; /* 新版本语法: Chrome 21+ */
}



.activityBcg {
    margin: 35px 0;
}
.activeTitle {
    text-align: center;
    font-size: 1.25rem;
    position: relative;
    margin-top: 1rem;
}
.activeTitle i {
    position: absolute;
    width: 20%;
    height: 5px;
    background-color: #fff;
    bottom: 0;
    left: 0;
    right: 0;
    margin: auto;
}
.activeTitle:after {
    content: '';
    width: 67%;
    display: block;
    margin: 10px auto;
    height: 1px;
    background-color: #fff;
}
.active-msg {
    width: 90%;
    margin: auto;
}
.active-msg h1 {
    font-size: 22px;
    display: inline-block;
    background-color: #FFEB75;
    color: #000;
    padding: 0 35px 0 10px;
    margin: 10px 0;
}
.active-msg a {
    color: #fff;
}
.active-msg p:not(:last-child) a:before {
    content: "";
    display: inline-block;
    width: 4px;
    height: 4px;
    background-color: #ccc;
    border-radius: 50%;
    position: relative;
    bottom: 3px;
    margin-right: 10px;
}
.active-msg .active-more {
    font-size: 1.1rem;
    margin: 1rem 0;
    float: right;
}
.active-msg p {
    line-height: 25px;
    font-size: .8rem;
}

.doctor-title-text, .media-title-text {
    line-height: 1;
    font-size: .8rem;
}
.doctor-title, .media-title {
    margin: 1rem 0;
    padding-bottom: 1rem;
    border-bottom: 1px solid #27408B;
    position: relative;
    margin-top: 2rem;
    font-size: .9rem;
}

.doctor-title h1, .media-title h1, .technology-title h1 {
	font-size: 1.6rem;
}
.doctor-title h1 span, .media-title h1 span {
	font-weight: 100;
	font-family: 'Adobe 黑体 Std';
	line-height: 0;
}
.doctor-title:after, .media-title:after, .technology-title:after {
	content: '';
	width: 20%;
	margin: auto;
	background-color: #5466A4;
	height: 5px;
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
}
.doctor-text, .media-text {
    text-align: left;
}

.doctor-detail, .doctor-msg, .doctor-msg2 {
    position: absolute;
    bottom: 0;
    background: rgba(49, 54, 66, .7);
    left: 0;
    right: 0;
    transition: all .5s ease;
}
.doctor-msg, .doctor-msg2 {
    text-align: center;
    padding-bottom: 6%;
}
.doctor-msg .doctor-position, .doctor-msg2 .doctor-position {
    color: #fff;
    margin-bottom: 5px;
    overflow: hidden;
    width: 100%;
    white-space: nowrap;
    text-overflow: ellipsis;
    font-size: .8rem;
}
.doctor-detail {
    color: #fff;
    top: 100%;
    z-index: 9;
    opacity: 0;
    padding-left: 5%;
    padding-right: 5%;
}
.doctor-name span {
    font-size: .9rem;
    margin-left: .6rem;
}
.doctor-msg .doctor-name:after, .doctor-msg2 .doctor-name:after {
    content: '';
    width: 20%;
    height: 3px;
    background: #fff;
    margin: auto;
    display: block;
    top: 5px;
    position: relative;
}
.doctor-name {
    color: #fff;
    font-size: 1.25rem;
    margin: 6% 0;
    font-weight: 600;
    margin-bottom: 5%;
    text-align: center;
}
.doctor-detail div {
    margin: 10% 0;
    font-size: .8rem;
}
.doctor-detail .be-good-porject {
    margin-top: 15%;
}
.doctor-detail .doctor-btn {
    position: absolute;
    left: 10%;
    bottom: 1rem;
    display: none;
}
.doctor-detail .doctor-btn a {
    color: #fff;
    border: 1px solid #fff;
    padding: .3rem 1rem;
    display: inline-block;
    font-size: .8rem;
}
.doctor-list li>div {
    position: relative;
    cursor: pointer;
    overflow: hidden;
}
.doctor-list {
	justify-content: space-between;
    -webkit-box-pack: justify;
    -webkit-justify-content: space-between;
    -ms-flex-pack: justify;
    -webkit-flex-wrap: wrap;
	flex-wrap: wrap;
    margin-bottom: 1rem;
    padding: 0 1rem;
}
.doctor-list li {
    width: 45%;
    margin-top: 2rem;
}
.local {
	margin: 15px 0;
}
.media-logo {
    padding-bottom: 2rem;
}

.media-text, .technology-text, .doctor-text, .media-text {
    width: 90%;
    font-size: .9rem;
    text-indent: 2rem;
    margin: auto;
}
.media-logo li {
	width: 90%;
	background: #fff;
    margin: auto;
	margin-top: 5%;
}
.doctor-title-text, .media-title-text, .technology-title-text {
    line-height: 1;
}
.doctor-title, .media-title, .content1-title, .technology-title {
    display: inline-block;
    margin: 30px 0;
    border-bottom: 1px solid #27408B;
    position: relative;
}

.section6{
	background-color: #09247c;
}
.section6-content{
	background-color: #071038 ;
	align-items: center;
	position: relative;
    margin-top: 2rem;
    padding-bottom: 2rem;
}

.section6-content-right{
    width: 80%;
    margin: auto;
    padding: 1rem 0;
}


.section6-input-text{
	justify-content: space-between;
    margin-top: 1rem;
}
.section6-input-text .must {
    color: #fff;
    margin: 5px 0;
}
.section6-input-text .section6-input-text-small:first-child {
    padding-right: 2.5%;
}
.section6-input-text .section6-input-text-small:last-child {
    padding-left: 2.5%;
}
.section6-input-text .section6-input-text-small {
	padding: 5px 0 0 0;
}

.section6-input-text input[type="text"] {
    height: 2rem;
    width: 100%;
    color: #555;
    border: 0;
    background-color: #fff;
    padding: .6rem;
    display: block;
    outline: none;
    box-sizing: border-box;
}

.section6-input-text .checkbox-box .checkbox-item span {
	color: #fff;
}

.section6-btn a {
    background: #2956b4;
    color: #fff;
    text-align: center;
    display: block;
    margin: 0 auto;
    margin-top: 2rem;
    padding: 4% 0;
}

.doctor-detail>div>p {
	margin: 5px 0;
}
.media-title, .technology-title {
    margin-top: 0;
    padding: 1rem;
}
.media-title-text {
    color: #9b9b9b;
}

#form1 {
    display: block;
}
.technology-img-box {
    margin: 1rem;
}
.doctor-second-text {
    font-size: 1.2rem;
    border: 1px solid #fff;
    margin: auto;
    display: inline-block;
    padding: 5px 15px;
    margin-top: 30px;
    width: 80%;
}
.section7 {
    background-color: #B90F0F;
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    padding: .5rem 0;
    z-index: 100;
}

.section7 h1 {
    font-size: 1.2rem;
}
.section7 input {
    outline: none;
    border: none;
    display: block;
    padding-left: 10px;
    color: #333;
    height: 1.5rem;
    width: 30%;
    margin-right: 5px;
}
.submit-btn {
    color: #fff;
    background-color: #E57C1E;
    font-size: 1rem;
    line-height: 2;
    border-radius: 5px;
    position: absolute;
    right: 3%;
    top: 30%;
    width: 4rem;
    text-align: center;
}
.clearfloat:after{display:block;clear:both;content:"";visibility:hidden;height:0}
.clearfloat{zoom:1}
.input-line {
    margin: .5rem 0;
}