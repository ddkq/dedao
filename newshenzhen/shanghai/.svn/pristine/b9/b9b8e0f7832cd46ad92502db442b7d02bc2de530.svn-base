.banner {
	margin-top: 120px;
}

input{
	border: 1px solid #000000;
}

.form-bg{
	background: #fff8ea;
}

form .age span, form .condition span {
	width: 25%;
	background-color: #999999;
	color: #000;
	font-size: 0.9rem;
}
form .condition span {
	width: 28%;
	height: 35px;
}
form .condition .bottom-span {
	width: auto;
}
form p {
	color: #000;
}

.active {
	background-color: #c11920 !important;
	color: #fff !important;
}

.submit-btn {
	border-radius: 10px;
  	background-color: rgb(199, 6, 6);
  	font-size: 1.2rem;
}

.top{
	background-color: #85521d;
	color: #fdedac;
}

.border-bottom{
	border-bottom:2px solid #000 !important;
}

.brown{
	color: #6c3c07;
}

.yel-bg{
	background-color: #fffeef
}

.swiper-pagination-bullet-active{
	background: #d20429;
}
.swiper-pagination-bullet{
	margin: 0 0.2rem;
	width: 12px;
	height: 12px;
	background: #f1b0b7;
}

.position-none{
	position: static;
}

.doctor{
	background: #fca823;
}

.bg-red{
	background: #a50708;
}

footer{
	bottom: 0;
	z-index: 99;
	max-width: 640px;
}

.w-40{
	width: 40%;
}
.w-60{
	width: 60%;
}

.bg-grey{
	background: #e5e5e5;
	color: #333333;
}


.bg-red1{
	background: #e80d0e;
}
