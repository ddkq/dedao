a {
	color: #333;
}
a:hover {
	text-decoration: none;
	color: #fff;
}
ul {
	margin: 0;
}
.font-11{
	font-size: 1.1rem;
}
.banner {
	margin-top: 120px;
}
.red{
	color: #c30d24;
}
.content-box-1 h3{
	background: #e6e6e6;
}
.p-num:before,.p-num:after{
	content: '';
	width: 20%;
	height: 1px;
	background: #fff;
	position: absolute;
	top: 30%;
}
.p-num:before{
	left: 0;
}
.p-num:after{
	right: 0;
}
.content-box-1 .container{
	background: #fede47;
}

.submit-btn{
	border-radius: 12px;
	background-color: rgb(255, 0, 0);
}
.content-box-3{
	background: #710002;
}
.copyright{
	background: #4f0101;
}
.cricle{
	background-color: rgb(145, 89, 93);
	border-radius: 50%;
	width: 8rem;
	height: 8rem;
	bottom: 3rem;
	left: 2rem;
	animation: move 2s .2s ease both infinite;
	-webkit-animation: move 2s .2s ease both infinite; /* Safari å’Œ Chrome */
}
.swt-btn{
	border-radius: 50%;
	background-color: rgb(228, 0, 15);
	border: 4px solid rgb(195, 20, 25);
	width: 7.5rem;
	height: 7.5rem;
	font-size: 2rem;
}
@keyframes move {
    0% { transform: scale(1); opacity: 1; }
    50% {transform: scale(1.2);  opacity: 1;  }
	100% { transform: scale(1);  opacity: 1; }
}

.bck-white{
	background: #fff;
	border-radius: 10px;
}
.bck-white p{
	font-size: 1.2rem;
}