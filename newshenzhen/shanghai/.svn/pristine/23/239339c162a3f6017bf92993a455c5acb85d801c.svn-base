@charset "utf-8";
*{margin:0; padding:0; border:0;}
html * {outline: 0; -webkit-text-size-adjust: none; -webkit-tap-highlight-color: transparent; }
li{list-style:none;}
a{text-decoration:none; color:#333}
body{max-width:640px;min-width:320px;margin:0 auto; font-family:"微软雅黑"; font-size: 100%;font-size:14px;}
input[type="submit"],input[type="reset"],input[type="date"]{-webkit-appearance: none;}
textarea,select,input{-webkit-appearance: none; -moz-appearance: none; -o-appearance: none; appearance: none;}
.fl{ float:left;}
.fr{ float:right;}
.b{ vertical-align:top; width:100%;}
.cfix:after{visibility:hidden;display:block;font-size:0;content:"";clear:both;height:0;line-height: 0;}
.cfix{*zoom:1;}
.notext{ text-indent: -9999px; overflow: hidden;}
.cl{clear: both;height: 0;width: 100%;font-size: 0;}

.banner {width: 100%; margin-bottom: 3%;margin-top: 105px;}
.banner img {width: 100%;}
.mtips{position: relative; width: 93.75%;margin: 0 auto 4% auto;}
.mtips .info{position: absolute;left: 18%; top: 50%; right: 2%; font-size: .7rem;line-height: 1.5;color: #333; -webkit-transform: translateY(-50%); -ms-transform: translateY(-50%); transform: translateY(-50%);}
.mtips .info a{color: #ff8b8b;}

.m{width: 93.75%;margin: 0 auto 4% auto; font-size: 1rem;line-height: 1.2; color: #333;}
.mhd,
.mbtn{margin-bottom: 4%;}
.mhd h3{background: url(../imgs/m1-hd.png) no-repeat; line-height: 2; text-align: center; font-size: 1.25rem; color: #42b8a2;}
.mhd h3 span{color: #ff8b8b;}
.mbg{padding: 4% 3.125%; margin: 0 auto 4%; background: #f1fffa;}

.m1 ul,
.m2 ul{margin-bottom: 1%;}
.m1 li,
.m2 li,
.m2 li{float: left;width: 30%; margin-bottom: 3%;}
.m1 li:nth-child(2),
.m1 li:nth-child(5),
.m2 li:nth-child(2){margin-left:5%;margin-right: 5%;}
.m1 .p1{padding:.35rem 0;line-height: 1; text-align: center; font-size: .9rem; color: #fff; background: #42b8a2;}
.m1 .p2{margin:5% 0 0 0;font-size: .85rem;color: #999;line-height: 1.3;}
.m1 .p2 strong{color: #333;}
.m2 .p1{margin-bottom: 3%;}
.m2 .mtips{margin-bottom: 0;}

.m3 .p1{line-height: 1.3; margin-bottom: 4%;}
.m3 .p1 span{display: inline-block; padding: .25rem 5px; margin-right: 1em; color: #fff; line-height: 1; border-radius: 0.17rem; background: #42b8a2;}
.m3 ul{margin-bottom: 2%;}
.m3 li{float: left;width: 48.333%; margin-bottom: 3%;}
.m3 li:nth-child(2n){float: right;}
.m3 li img{width: 96.5%; display: block;margin: 0 auto;}
.m3 li .p1{padding: .25rem 0; margin-bottom: 0;line-height: 1; text-align: center; font-size: 1rem; color: #fff; background: #42b8a2;}
.m3 li .p2{width: 96.5%; display: block;margin: 0 auto;color: #999;padding:3% 2%; background: #f1f1f1;height: 5rem; -webkit-box-sizing: border-box;box-sizing: border-box;}
.m3 li .p2 strong{color: #333;}
.m3 .mbtn{margin-bottom: 0; }

.m4 .list1{margin-bottom: 1%;}
.m4 .list1 li{float: left;width: 32%; margin-bottom: 3%; text-align: center;}
.m4 .list1 li:nth-child(2){margin-right:2%;margin-left:2%;}
.m4 .list1 .p1{padding: 0.25rem 0; border-radius: 0.2rem 0.25rem 0 0; line-height: 1; text-align: center; font-size: 1rem; color: #fff; background: #42b8a2;}
.m4 .list1 .p2{height: 2.6rem;line-height: 1.3; border-radius: 0 0 0.25rem 0.25rem; background: #f1f1f1;font-size: .8rem;padding: .25rem 0;}
.m4 .list1 li:nth-child(1) .p2{ line-height: 1.3;}

.m4 .list2{}
.m4 .list2 li{float: left; position: relative; width: 31%;height: 7rem;background: #f1f1f1; border-radius: 0.25rem; overflow: hidden;}
.m4 .list2 li:nth-child(1){width: 6.5rem;}
.m4 .list2 li:nth-child(2){margin: 0 0.05rem;border-right: 1px solid #fff;}
.m4 .list2 .info{ padding: .75rem 0 0 0; text-align: center; font-size: .85rem; line-height: 1.5; -webkit-box-sizing: border-box; -ms-box-sizing: border-box; -o-box-sizing: border-box; box-sizing: border-box;}
.m4 .list2 .info span{text-decoration: line-through; color: #999; font-size: 0.2rem;}
.m4 .list2 b{display: block; line-height: 2; text-align: center; font-size: 1rem; color: #fff; background: #42b8a2;}

.m5 .list1{margin-bottom: 4%;}
.m5 .list1 li{padding:2% 0;border-bottom: 1px dotted #e1e1e1;}
.m5 .list1 li .title{font-size: 1rem;color: #333; line-height: 1.5}
.m5 .list1 li .info{font-size: .85rem;color: #999;}

#wpzj .con{width: 100%;}

.m7{margin-bottom: 1rem;}
.m7 .list1 li{float:left;width: 48.5%;height: 7.5rem; margin-bottom: 3%;}
.m7 .list1 li:nth-child(2n){float: right;}
.m7 .list1 li .title{line-height: 1;padding: .5rem 0; text-align: center; font-size: .95rem; color: #fff; background: #42b8a2; border-radius: 0.25rem 0.25rem 0 0;}
.m7 .list1 li .info{height: 5.4rem; padding: 0.75rem 0 0 0;font-size: .75rem; text-align: center; -webkit-box-sizing: border-box;-ms-box-sizing: border-box;-o-box-sizing: border-box;box-sizing: border-box; border: 1px dotted #42b8a2;border-top:0; border-radius: 0 0 0.25rem 0.25rem;}


























