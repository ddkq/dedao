<admintpl file="header" />
<link href="__PUBLIC__/simpleboot/css/from.css" rel="stylesheet">
</head>
<body>
	<div class="wrap js-check-wrap">
		<ul class="nav nav-tabs">
			<li><a href="{:U('AdminTables/index')}">表单管理</a></li>
			<li class="active"><a href="javascript:;">数据查看</a></li>
		</ul>
		<div class="table-actions">
			<!--<button class="btn btn-primary btn-excel-import">导入</button>-->
			<button class="btn btn-primary btn-excel">导出</button>
		</div>
		<form class="well form-search" method="post" action="{:U('AdminTables/view_data')}">
			时间：
			<input type="text" name="start_time" class="js-date" value="{$formget.start_time|default=''}" style="width: 80px;" autocomplete="off">-
			<input type="text" class="js-date" name="end_time" value="{$formget.end_time}" style="width: 80px;" autocomplete="off"> &nbsp; &nbsp;
			姓名： 
			<input type="text" name="keyword" style="width: 200px;" value="{$formget.keyword}">
			电话：
			<input type="text" name="phone" style="width: 200px;" value="{$formget.phone}">
			url:
			<input type="text" name="page" style="width: 200px;" value="{$formget.page}">
			
			<input type="submit" class="btn btn-primary" value="搜索" />
		</form>
		<form class="js-ajax-forms" action="" method="post">
			<table class="table table-hover table-bordered table-list" style="table-layout: fixed;">
				<thead>
					<tr>
						<th width="3%">id</th>
						<th width="12%">当前页</th>
						<th width="7%">ip</th>
						<th width="7%">提交时间</th>
						<th width="7%">姓名</th>
						<th width="7%">电话</th>
						<th width="7%">症状</th>
						<th width="7%">年龄</th>
						<th width="7%">性别</th>
						<th width="7%">预约时间</th>
						<th width="7%">其他信息</th>
						<th width="7%">对应表单</th>
						<th width="7%">状态</th>
						<th width="7%">操作</th>
					</tr>
				</thead>
				<foreach name="posts" item="vo">
					<php>$content1 = json_decode($vo['content'],true);</php>
				<tr>
					<td width="3%">{$vo.id}</td>
					<td width="12%" style="overflow: auto;">{$vo.page}</td>
					<td width="7%">{$vo.ip}</td>
					<td width="7%">{$vo.createtime}</td>
					<td width="7%">{$content1.name}</td>
					<td width="7%">{$content1.phone}</td>
					<td width="7%">{$content1.symptom}</td>
					<td width="7%">{$content1.age}</td>
					<td width="7%">{$content1.gender}</td>
					<td width="7%">{$content1.time}</td>
					<td width="7%">{$content1.etc}</td>
					<td width="7%">{$vo[tables][name]}</td>
					<td width="7%">
						<if condition="$vo[status] eq 0">
							<a href="{:U('AdminTables/determine',array('id'=>$vo['id']))}" class="js-ajax-dialog-btn">未处理（点击处理）</a>
						<else />
							已处理
						</if>
					</td>
					<td>
						<if condition="$vo[tables_id] eq 44">
							<a href="{:U('/Questionnaire/AdminQuestionnaire/view_data',array('id'=>$vo['id']))}">问卷详情</a>	
						</if>
						<a href="{:U('AdminTables/view_data_delete',array('id'=>$vo['id']))}" class="js-ajax-delete">删除</a>
					</td>
				</tr>
				</foreach>
				<tfoot>
					<tr>
						<th width="3%">id</th>
						<th width="12%">当前页</th>
						<th width="7%">ip</th>
						<th width="7%">提交时间</th>
						<th width="7%">姓名</th>
						<th width="7%">电话</th>
						<th width="7%">症状</th>
						<th width="7%">年龄</th>
						<th width="7%">性别</th>
						<th width="7%">预约时间</th>
						<th width="7%">其他信息</th>
						<th width="7%">对应表单</th>
						<th width="7%">状态</th>
						<th width="7%">操作</th>
					</tr>
				</tfoot>
			</table>
			<div class="pagination">{$Page}</div>
		</form>
		
		<style>
			#calroot{z-index: 10001;}
		</style>
		<div class="modal-wrap excel" style="display: none;">
			<div class="modal">
				<div class="modal-content">
					<a class="modal-close">
	                    <span class="modal-close-x"></span>
	                </a>
	                <div class="modal-header">
	                    <div class="modal-title">选项设置</div>
	                </div>
	                <div class="modal-body">
	                    <div slot="content" class="radio-modal">
							<form class="" method="post" action="{:U('AdminTables/data_excel')}">
								时间：
								<input type="text" name="start_time" class="js-datetime" value="{$formget.start_time|default=''}" style="width: 80px;" autocomplete="off">-
								<input type="text" class="js-datetime" name="end_time" value="{$formget.end_time}" style="width: 80px;" autocomplete="off"> &nbsp; &nbsp;
								<br/>
								表单地址：
								<input type="text" name="page" style="width: 200px;" value="{$formget.page}" placeholder="请填写url地址" />
								<br/>
								ip：
								<input type="text" name="ip" style="width: 200px;" value="{$formget.ip}" placeholder="请填写ip地址" />
								<br/>
								对应表单名
								<select class="select_2" name="tables_id">
									<option value='0'>全部</option>
									<volist name="tables" id="vo">
										<option value="{$vo.id}">{$vo.name}</option>
									</volist>
								</select>
								<input type="submit" class="btn btn-primary" value="导出" />
							</form>
						</div>
					</div>
				</div>
			</div>
		</div>
		
		<!--<div class="modal-wrap excel-import" style="display: none;">
			<div class="modal">
				<div class="modal-content">
					<a class="modal-close">
	                    <span class="modal-close-x"></span>
	               </a>
	                <div class="modal-body">
	                    <div slot="content" class="radio-modal">
							<form class="" method="post" action="{:U('AdminTables/imports')}">
								<input type="hidden" name="excel" id="thumb" value="">
								<a href="javascript:upload_one('导入excel','#thumb','file');">选择文件</a>
								<input type="submit" class="btn btn-primary" value="导入" />
							</form>
						</div>
					</div>
				</div>
			</div>
		</div>-->
		
	</div>
		
	</div>
	<script src="__PUBLIC__/js/common.js"></script>
	<script>
		function refersh_window() {
			var refersh_time = getCookie('refersh_time');
			if (refersh_time == 1) {
				window.location = "{:U('AdminTables/index',$formget)}";
			}
		}
		setInterval(function() {
			refersh_window();
		}, 2000);
		
		
		$('body').on('click', '.btn-excel', function() {
			$('.modal-wrap').fadeIn();
		});
		$('.modal-close-x').on('click', function() {
			$('.modal-wrap').fadeOut();
		})
		/*$('body').on('click', '.btn-excel-import', function() {
			$('.modal-wrap').fadeIn();
		});*/
	</script>
</body>
</html>