img{
	max-width: 100%;
}
.red {
	color: #cc1515;
}
.other {
	border: none;
	border-bottom: 1px solid #999;
}
.form input {
	margin-right: .5rem;
    position: relative;
    top: 1.4px;
}
p {
	font-size: .9rem;
}
h5 {
	font-size: 1.1rem;
}
.form1 input {
    -webkit-appearance: none;
    border-radius: 0;
}
.form1 input{
	border: 0;
	border-bottom: 1px solid #999999 !important;
}
.submit-btn{
	background: #eed790;
	color: #fff;
	display: block;
	width: 50%;
	padding: 0.5rem;
	text-align: center;
	margin: 2rem auto;
}
table i{
	display: block;
    border: 1px solid #999;
    width: 0.5rem;
    padding: 0.5rem;
    margin: auto;
}
table i.check{
	background: #eed790;
	border-color: #eed790;
}
table input{
	display: none;
}
