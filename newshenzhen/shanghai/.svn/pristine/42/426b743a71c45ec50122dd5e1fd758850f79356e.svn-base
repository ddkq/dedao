			mySwiper.slideTo(inx, 500, false);
		});
		var mySwiper2 = new Swiper('.case', {
			simulateTouch : false,
			navigation: {
		      	nextEl: '.arrow-right',
		      	prevEl: '.arrow-left',
		    },
		    loop: true,
		});
		var mySwiper3 = new Swiper('.doctor-tab .swiper-container', {
			simulateTouch : false,
			navigation: {
		      	nextEl: '.arrow-right2',
		      	prevEl: '.arrow-left2',
		    },
		    slidesPerView : 3,
			spaceBetween : 20,
		});
		$('.arrow-right2').on('click', function() {
			var inx = $('.doctor-tab-active').parent().parent().index();
			if(inx != 3) {
				$('.doctor-tab .doctor-tab-active').removeClass('doctor-tab-active');
				$('.doctor-tab .swiper-slide').eq(inx+1).children().children().addClass('doctor-tab-active');
				mySwiper.slideTo(inx+1, 500, false);
			}
		});
		$('.arrow-left2').on('click', function() {
			var inx = $('.doctor-tab-active').parent().parent().index();
			if(inx != 0) {
				$('.doctor-tab .doctor-tab-active').removeClass('doctor-tab-active');
				$('.doctor-tab .swiper-slide').eq(inx-1).children().children().addClass('doctor-tab-active');
				mySwiper.slideTo(inx-1, 500, false);
			}
		});
		$('.page').val(window.location.href);
		$('body').on('click', '.submit-btn', function() {	
			var name = $(".input-name").val();
			var re1 = /^[\u4E00-\u9FA5]{1,6}$/; 
			if(name == null){
				alert("姓名不能为空"); 
				return false; 
			}else if(name != null){
				if (!re1.test(name)){ 
					alert("姓名格式不正确"); 
					return false; 
				}	
			}
			
			var phone = $(".input-phone").val();
			var re2 = /^((13[0-9]{1})|(15[0-9]{1})|(18[0-9]{1}))+(\d{8})$/;
			if(phone == null){
				alert("电话不能为空"); 
				return false; 
			}else if(phone != null){
