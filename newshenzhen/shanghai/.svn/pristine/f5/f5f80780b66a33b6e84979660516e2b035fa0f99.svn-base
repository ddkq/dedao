* {
	font-family: '思源黑体 CN' !important;
}
a:hover {
	text-decoration: none;
}
.fa {
	font-family: 'FontAwesome' !important;
}
.text-indent {
	text-indent: 2rem;
}
.font-21 {
	font-size: 21px;
}
.banner {
	margin-top: 230px;
}
.banner .swt-btn {
	font-size: 30px;
	border: 2px solid #fff;
	bottom: 30%;
    left: 27%;
    width: 17%;
    animation: anim 4s infinite;
    padding: 5px 0;
}
@keyframes anim {
	0% {bottom: 30%;}
	50% {bottom: 25%;}
	100% {bottom: 30%;}
}
.content-1 {
	background-color: #F1F1F1;
}
.content-1 .container {
	background-color: #fff;
}
.content-1-banner-text {
	font-size: 20px;
  	background-color: rgba(7, 29, 110, .702);
    bottom: 0;
    z-index: 2;
    left: 0;
    right: 0;
    width: 86%;
    margin: auto;
}
.content-1 .arrow {
	display: inline-block;
	width: 25px;
	height: 20px;
	background-color: rgba(255, 255, 255, .8);
  	color: rgb(81, 81, 81);
  	line-height: 1;
  	cursor: pointer;
    right: -18%;
}
.swt-btn {
	border-radius: 60px;
	background-color: #0A2070;
}
.content-1-tab p {
	color: #fff;
}
.content-1-tab .active-tab p, .content-4-text, .content-title p, .content-2-swt-text li, .content-4-banner p {
	color: #515151;
}
.content-1-tab .index-icon {
	background: url(imgs/icon.png);
    width: 52px;
    height: 42px;
}
.content-1-tab>div {
	background-color: #0A2070;
	width: 31.5%;
	cursor: pointer;
}
.content-1-tab div.active-tab {
	background-color: #F1F1F1;
}
.content-1-tab>div:first-child .index-icon {
	background-position: -53px 0;
}
.content-1-tab>div:nth-child(2) .index-icon {
	background-position: -53px -46px;
}
.content-1-tab>div:nth-child(3) .index-icon {
	background-position: -53px -88px;
}
.content-1-tab>div:first-child.active-tab .index-icon {
	background-position: 0 0;
}
.content-1-tab>div:nth-child(2).active-tab .index-icon {
	background-position: 0 -46px;
}
.content-1-tab>div:nth-child(3).active-tab .index-icon {
	background-position: 0 -88px;
}
.content-1-tab .index-icon:after, .content-1-tab .index-icon:before {
	content: '';
  	background-color: #fff;
	width: 50px;
	height: 1px;
	display: inline-block;
	position: absolute;
    top: 49%;
}
.content-1-tab .index-icon:before {
	left: 16%;
}
.content-1-tab .index-icon:after {
	right: 16%;
}
.content-1-tab>div.active-tab .index-icon:after, .content-1-tab>div.active-tab .index-icon:before {
	background-color: rgb(7, 29, 110);
}
.content-1-img>div:before {
	content: '';
	position: absolute;
 	background-color: rgba(7, 29, 110, .988);
  	width: 370px;
  	height: 230px;
    left: 0%;
    bottom: -10%;
}
.content-title h1 {
	font-size: 48px;
}
.content-title p {
	font-size: 32px;
}
.content-2-banner {
	background: url(imgs/content-2.png);
	background-size: contain;
}
.content-2-text {
	bottom: 0;
  	background-color: rgba(7, 29, 110, .702);
  	width: 69.5%;
    left: 0;
    right: 0;
    margin: auto;
    z-index: 1;
}
.arrow-left, .arrow-right {
    height: 50px;
    width: 50px;
    position: absolute;
    top: 45%;
    z-index: 99;
    border-radius: 50%;
    background-color: #fff;
    cursor: pointer;
}
.arrow-right {
    left: 89%;
}
.arrow-left {
    right: 89%;
}
.arrow-right .arrow {
    transform: rotate(225deg);
    right: 0;
}
.arrow-left .arrow {
    transform: rotate(45deg);
    right: -12%;
}
.arrow {
    width: 15px;
    height: 15px;
    display: inline-block;
    border-bottom: 2px solid rgb(19, 80, 134);
    border-left: 2px solid rgb(19, 80, 134);
    position: absolute;
    top: 0;
    bottom: 0;
    margin: auto 20px;
    transition: all .2s linear;
}
.content-2-swt-text {
	background-color: #F6F6F6;
}
.content-2-swt-text li {
	width: 33%;
	font-size: 18px;
	font-weight: 600;
}
.content-2-swt-text .active-text {
	color: #135086;
}
.doctor-name, .content-4 .content-4-banner-text .title, .content-title h1, .content-1 h1, .content-4 h4 {
	color: #071d6e;
}
.doctor-name span:before {
	content: '';
    height: 30px;
    width: 1px;
    background: #071d6e;
    display: inline-block;
    position: absolute;
    bottom: 0;
    left: 0;
    transform: rotate(15deg);
}
.content-3-banner-bcg {
	bottom: 0;
}
.doctor-text {
	font-size: 18px;
	line-height: 1.8;
	font-weight: 300;
	min-height: 224px;
}
.doctor-msg li {
	border-radius: 50%;
	border: 2px solid #fff;
    line-height: 1;
    padding: 20px 0;
    width: 97px;
}
.top-text {
	font-size: 18px;
}
.content-3 .index-icon {
	background: url(imgs/icon.png);
	width: 22px;
    height: 25px;
    vertical-align: sub;
}
.content-3 .doctor-btn .swt-btn .index-icon {
	background-position: -27px -130px;
}
.content-3 .doctor-btn .swt-btn {
	background-color: rgba(0, 0, 0, 0);
}
.content-3 .doctor-btn a {
	border: 2px solid #fff;
    width: 29%;
    border-radius: 60px;
    color: #fff;
}
.content-3 .doctor-deatil .index-icon {
	background-position: -82px -130px;
}
.content-3 .doctor-btn .swt-btn:hover, .content-3 .doctor-deatil:hover {
	color: #135086;
	background: #fff;
}
.content-3 .doctor-btn .swt-btn:hover .index-icon {
	background-position: 0 -130px;
}
.content-3 .doctor-btn .doctor-deatil:hover .index-icon {
	background-position: -54px -130px;
}
.page {
    right: 4%;
    top: 32%;
}
.content-3 .arrow-left, .content-3 .arrow-right {
	top: 13%;
}
.content-3 .arrow-left, .content-3 .arrow-right, .content-4 .arrow-left, .content-4 .arrow-right {
	border: 2px solid #071D6E;
}
.content-3 .arrow-left {
	right: 91%;
}
.content-3 .arrow-right {
	left: 91%;
}
.content-3 .arrow, .content-4 .arrow {
  	border-color: rgb(7, 29, 110);
  	width: 11px;
  	height: 11px;
}
.content-3 .arrow-left .arrow, .content-4 .arrow-left .arrow {
	right: -9%;
}
.content-3 .arrow-left:hover, .content-3 .arrow-right:hover, .content-4 .arrow-left:hover, .content-4 .arrow-right:hover {
	background: #071D6E;
}
.content-3 .arrow-left:hover .arrow, .content-3 .arrow-right:hover .arrow, .content-4 .arrow-left:hover .arrow, .content-4 .arrow-right:hover .arrow {
	border-color: #fff;
}
.content-4-banner {
	background: #F6F6F6;
}
.right-img {
	width: 45%;
}
.left-text {
    width: 60.5%;
}
.content-4 h4 {
	border-bottom: 2px solid #071d6e;
}
.before, .after {
	background: #071D6E;
	right: 0;
	bottom: 0;
}
.content-4 .arrow-left {
	right: 93%;
}
.content-4 .arrow-right {
	left: 93%;
}
.content-4 .swt-btn-list .swt-btn {
	font-size: 32px;
	border: 2px solid #071D6E;
	color: #071d6e;
	background: #fff;
	width: 36%;
}
.content-4 .swt-btn-list .active-btn {
	background: #071D6E;
	color: #fff;
}
.content-5 {
	background: #F6F6F6;
}
.symptom-list>div {
	width: 32%;
	cursor: pointer;
	color: #717171;
}
.symptom-list>div i {
	width: 15px;
	height: 15px;
	border: 1px solid #717171;
}
.symptom-list {
	width: 60%;
}
.input-list {
	width: 38%;
}
.active-symptom i {
	background: #071D6E;
}
.input-list input {
	border-radius: 15px;
	width: 70%;
  	border: 1px solid rgb(81, 81, 81);
}
.input-list input:focus {
	border-color: #071D6E;
}
.submit-btn {
	border-radius: 60px;
	background: #071D6E;
	width: 70%;
}
.content-4 .swt-btn:hover {
	color: #fff!important;
}