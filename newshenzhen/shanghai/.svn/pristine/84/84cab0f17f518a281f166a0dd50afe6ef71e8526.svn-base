.banner {
	margin-top: 120px;
}
.content-form{
	background: #ecf9ff;
}
form input {
	border: 1px solid #000;
}
.submit-btn{
	border-radius: 20px;
	background-color: rgb(202, 7, 34);
	font-size: 1.2rem;
}
.blue{
	color: #36a9f7
}

.swiper-pagination-bullets{
	position: static;
}
.swiper-pagination-bullet{
	width: 16px;
	height: 16px;
	background: #b3b3b3;
	margin-right: 1.5rem;
	opacity: 1;
}
.swiper-pagination-bullet-active{
	background: #192956;
}

.border-bottom{
	border-style: dashed !important;
	border-color: #37a8f8 !important;
}
.w-20{
	width: 20%;
}
.font-8{
	font-size: 0.9rem;
}
.w-40{
	width: 40%;
}
.w-60{
	width: 60%;
}
.content-list{
	background: #c4eaff;
}
.h5, h5 {
    font-size: 1.2rem;
}
.w-35{
	width: 35%;
}
.w-65{
	width: 65%;
}
