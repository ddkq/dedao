@charset "utf-8";
@import url(common.css);
html{font-size:62.5}
body,h1,h2,h3,h4,h5,h6,hr,p,blockquote,dl,dt,dd,ul,ol,li,pre,form,fieldset,legend,button,input,textarea,th,td.img{border:none;list-style-type:none; margin:0; padding:0;}
h1,h2,h3,h4,h5,h6{font-size:100%;}
em{font-style:normal;}
ul,ol,li{list-style:none;}
img{ border:0; margin:0; padding:0;}
input,img{vertical-align:middle;}
body,html{margin:0 auto;overflow-x:hidden;}
*{margin:0;padding:0;text-shadow:none;-moz-box-sizing:border-box;-webkit-box-sizing:border-box;box-sizing:border-box;}
.clear{height:0;clear:both;overflow:hidden}
.clearfix:after,.clear:before{display:table;content:'';}
.clearfix:after{clear:both;overflow:hidden;}
.clearfix{zoom:1;}
span,em,img{display:inline-block;}
body { margin:0; padding:0; margin-bottom:10px}
a,a:visited{ padding:0; margin:0;}
div{ padding:0; margin:0;}

.ry img{ display:block;}
.wrap{ min-width:320px; max-width:640px; margin:0 auto;margin-top: 105px;}
.wrap .b{ width: 100%; vertical-align:top;}

.ban{ margin-bottom:30px;}
.pk{ margin:10px 0;}
.pk h3{ color: #FDB5CE; padding-left:2%; font-size:14px; }
.pk strong{ color: #23BDF1; font-size: 18px;padding:1% 0 2% 2%; display: inline-block;}
.yc{ margin:20px 0;}
.zx{ width:70%; margin: 4% auto; display: block}

.hg{ height:10px; clear: both;}
.fz{ width:80%; margin:0 auto 2%; border-radius: 5px; border:1px solid #FF356D; text-align: center; font-size: 16px;}
.fz p{ color:#FF356D; padding: 2% 0}
.fz .wx{ background-color:#FF356D; color:#fff; padding: 2% 0;}

.fzz{ width:80%; margin:0 auto 2%; border-radius: 5px; border:1px solid #FF356D; text-align: center; font-size: 16px;}
.fzz p{ color:#FF356D; padding: 2% 0}
.fzz .wx{ background-color:#FF356D; color:#fff; padding: 2% 0;}

h2.acc_trigger {padding: 0;	margin: 0 0 5px 0;height: 46px;	line-height: 46px;width:100%;font-size: 2em;float: left; background-color:#EEFBFF;}
h2.acc_trigger span{ padding-right:5px; font-size: 16px;}
h2.acc_trigger a {color:#23BDF1;text-decoration: none;display: block;padding: 0 0 0 20px;}
h2.acc_trigger a:hover {color: #fff; background-color:#23BDF1;}
h2.active {background-position: left bottom; background-color: #23BDF1;}
h2.active a{ color:#fff;}
.acc_container {margin: 0 0 5px; padding: 0;overflow: hidden;font-size: 1.2em;width:100%;clear: both;-webkit-border-bottom-right-radius: 5px;-webkit-border-bottom-left-radius: 5px;	-moz-border-radius-bottomright: 5px;-moz-border-radius-bottomleft: 5px;	border-bottom-right-radius: 5px;border-bottom-left-radius: 5px; }
.acc_container .block {	padding: 20px;}