@charset "utf-8";
*{margin:0; padding:0; border:0; box-sizing: border-box;}
html * {outline: 0; -webkit-text-size-adjust: none; -webkit-tap-highlight-color: transparent; }
li{list-style:none;}
a{text-decoration:none; color:#333}
body{max-width:640px;min-width:320px;margin:0 auto; font-family:"微软雅黑"; font-size: 100%;font-size:14px;}
input[type="submit"],input[type="reset"],input[type="date"]{-webkit-appearance: none;}
textarea,select,input{-webkit-appearance: none; -moz-appearance: none; -o-appearance: none; appearance: none;}
.fl{ float:left;}
.fr{ float:right;}
.b{ vertical-align:top; width:100%;}
.cfix:after{visibility:hidden;display:block;font-size:0;content:"";clear:both;height:0;line-height: 0;}
.cfix{*zoom:1;}
.notext{ text-indent: -9999px; overflow: hidden;}
.cl{clear: both;height: 0;width: 100%;font-size: 0;}
body{background: #f3f3f3;}
.m{width: 93.75%;margin: 5% auto 0;}
.p1{color: #313131;}
.m1 .list li{float: left;width: 33.33333%;padding: 4% 2% 0 2%;}
.m1 .list li p{padding-top: 10%;font-size: 0.8rem;color: #313131;line-height: 1.3;text-align: center;}
.m1 .btn{padding-top: 4%;}
.zhd{text-align:center;color:#003287 !important; padding:0.1rem 0 0.5rem 0 !important; background: url("../imgs/zhd.jpg") left bottom no-repeat;background-size: 100% 0.25rem;}
.zhd span{font-size: 1.1rem !important;}
.zhd h2{font-size: 1rem !important;color:#003287 !important;font-weight: normal !important;}

.m2 ul{width: 95%;margin: 5% auto 0;}
.m2 li{float:left; width: 33.33333%;}
.m2 li img{display: block; width: 85%;margin: 0 auto;}
.m2 li p{padding-top: 5%;font-size: 0.2rem;color: #313131;text-align: center;}
.m2 .intro{color: #313131;padding: 3%; margin-top: 4%; background: #dbe0e8;}
.m2 .pic{margin-top: 4%;}

.m3 .p1{padding: 3%;}
.m3 .pic img{display: block;}

.m3 .picbox{overflow: hidden;padding-left: 3.1rem;border: 1px solid #6986b6;}
.m3 .picbox img{float: left;width:9.343rem;height: 7.531rem;margin-left: -3.1rem;margin-right: 2%;}
.m3 .picbox h3{padding-top:5%;font-size: 0.28rem;font-weight:normal;color: #003287;}
.m3 .picbox h3 strong{border-bottom: 2px solid #2d569b;}
.m3 .picbox h4{font-size: 0.24rem;font-weight:normal;color: #313131;line-height: 2.5;}
.m3 .picbox p{color: #313131;font-size: 0.2rem;line-height: 1.35;}

.m3 h1{margin-top:4%;font-size: 0.26rem;color: #003287;font-weight: normal; text-align: center;}
.m3 .tab-hd{width: 96%;margin: 0 auto 6%;}
.m3 .tab-hd li{float: left;width: 50%;}
.m3 .tab-hd li img{display: block;width: 94%;margin: 6% auto 0;}
.m3 .btn{padding-top: 4%;}


.m4 .p1{padding: 3% 1%;}
.m4 li,.m5 .tags li{float: left; width: 33.33333%;padding: 3px;}
.m4 li a{display: block;height:1.5rem;text-align: center; font-size: 0.2rem; color:#003287;line-height: 2; background-color: #d2d7e0;}
.m4 li:nth-child(1) a,
.m4 li:nth-child(6) a{ background-color: #c0d2e5;}
.m4 li:nth-child(2) a,
.m4 li:nth-child(4) a,
.m4 li:nth-child(3) a{ background-color: #c9d6ea;}

.m4 .btn{padding-top: 3%;}

.m5 .zbd{position: relative;}
.m5 .zj-intro{ position: absolute; right: 0;top: 10%; width: 49%;}
.m5 h2{font-size: 0.36rem; font-weight: bold; color: #000; }
.m5 h3{font-size: 0.28rem; font-weight: normal; color: #000; line-height: 0.4rem;}
.m5 .p1{margin-top: 3%;color: #003287;}
.m5 .p2{margin-top: 5%;color: #313131;}
.m5 .p2 span{display:block;font-size: 0.28rem; line-height: 0.4rem; padding-bottom: 2%;}
.m5 .tags{width: 95%; margin: 5% auto 0 auto;padding-top: 5%;border-top: 1px solid #003287;}
.m5 .tags ul{margin: 0 -5px;}
.m5 .tags a{display: block;height:2rem;padding:0.1rem 0;font-size: 0.2rem;line-height:1.25;text-align: center; color: #fff; background-color: #91a5c8;}
.m5 .btn{margin-top: 5%;}

.m6 .zhd{margin-bottom: 0.2rem;}
.m6 .time{font-size: 0.2rem;color: #959595;line-height: 0.4rem;text-align: center;}
.m6 .list li{float: left;width: 50%;padding: 2%;text-align: center;}
.m6 .list li a{display: block;padding:3%;border:1px solid #7792bf;background: #fff;}
.m6 .list li img{border:1px solid #7792bf;}
.m6 .p2{padding-top:6%;color: #003287;font-size: 0.2rem; line-height: 0.26rem;}
.m6 .p3{color: #6d22ac;font-size: 0.22rem; font-weight: bold; line-height: 0.26rem;}

.m7{margin-bottom: 5%;}
.m7 .bd img{margin-top: 2%;}

#wpzj .con{width: 100% !important;}






















